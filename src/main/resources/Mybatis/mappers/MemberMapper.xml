<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shinhan.memento.dao.MemberDAOInterface">

	<select id="selectMemberById" parameterType="int" resultType="com.shinhan.memento.model.Member">
		select *
		from member
		where member_id=#{memberId}

	</select>
</mapper>